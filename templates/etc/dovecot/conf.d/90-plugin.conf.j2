{% if dovecot_plugin.plugins is defined and
      dovecot_plugin.plugins | count > 0 %}
  {% if dovecot_plugin.plugins | type == "list" %}
    {% for plugin in dovecot_plugin.plugins %}
plugin {
  {{ plugin.keys() | first }} = {{ plugin.values() | first }}
}
    {% endfor %}
  {% endif %}
{% endif %}
